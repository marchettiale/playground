{
  "name": "playground",
  "version": "1.1.0",
  "scripts": {
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headless": "playwright test --headed false",
    "test:debug": "playwright test --debug",
    "test:coverage": "playwright test --coverage",
    "test:codegen": "playwright codegen",
    "test:report": "npx playwright show-report"
  },
  "keywords": [
    "test",
    "playwright",
    "playground",
    "typescript"
  ],
  "author": "Bruno Machado, Alessandro Marchetti",
  "license": "MIT",
  "description": "Framework de testes para a página de desafios do Playground, utilizando Playwright para automação de testes end-to-end.",
  "devDependencies": {
    "@playwright/test": "^1.48.2",
    "@types/node": "^22.9.0"
  }
}
